{
  "name": "crisiscleanup-4-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18 <20",
    "pnpm": "^8"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/CrisisCleanup/crisiscleanup-4-web.git"
  },
  "scripts": {
    "postinstall": "is-ci || husky install",
    "dev": "vite --port 8080",
    "build": "pnpm run typecheck && pnpm run build:app",
    "build:app": "cross-env NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "build:app:maintenance": "cross-env VITE_APP_ENTRY=maintenance vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "lint:errors": "pnpm run lint --quiet",
    "lint:format": "pnpm run lint:fix && pnpm run format",
    "format": "prettier --write \"**/*.{vue,ts?(x),js?(x),json,md,y?(a)ml}\"",
    "clean": "pnpm dlx rimraf dist \"**/coverage\"",
    "repair:clean-modules": "pnpm dlx rimraf node_modules",
    "repair:prune-pnpm": "pnpm run clean && pnpm run repair:clean-modules && pnpm store prune",
    "pw": "pnpm exec playwright",
    "e2e:install": "pnpm run pw install --with-deps",
    "e2e:report": "pnpm run pw show-report",
    "e2e:gen": "pnpm run pw codegen",
    "test": "pnpm run test:cov",
    "test:unit": "vitest",
    "test:e2e": "pnpm run pw test",
    "test:e2e:development": "pnpm run test:e2e --grep '@development' --timeout 180000",
    "test:e2e:staging": "pnpm run test:e2e --grep '@staging'",
    "test:e2e:production": "pnpm run test:e2e --grep '@production'",
    "test:e2e:ui": "pnpm run pw test --ui",
    "test:cov": "pnpm run test:unit --coverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/vue-fontawesome": "^3.0.3",
