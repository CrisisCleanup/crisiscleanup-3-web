    "vue-undo-redo-stack": "^1.0.0",
    "vuex-mock-store": "0.0.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "lint-staged": {
    "*.{js,vue}": [
      "yarn format",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged && yarn test:unit"
    }
  }
}
