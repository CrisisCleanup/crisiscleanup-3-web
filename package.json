{
  "name": "crisiscleanup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode local",
    "serve-dev": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build",
    "lint-fix": "vue-cli-service lint --ignore-path .gitignore .",
    "lint-staged": "lint-staged",
    "lint": "vue-cli-service lint --no-fix --ignore-path .gitignore .",
    "prettify": "prettier --write {.,**}/*.{js,vue}",
    "deploy-staging": "vue-cli-service build --mode staging && vue-cli-service s3-deploy",
    "compile-icons": "./template-compiler.js",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "cross-env NODE_ENV=test jest --coverage",
    "test:unit-fix": "cross-env NODE_ENV=test jest -u",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:e2e": "cross-env NODE_ENV=test cypress run",
    "test:cy": "cross-env NODE_ENV=test cypress run --browser chrome",
    "test:percy": "cross-env NODE_ENV=test percy exec -- yarn run test:cy",
    "cy:open": "cross-env NODE_ENV=test cypress open"
  },
