    "vite-plugin-vue-inspector": "^3.6.0",
    "vitest": "^0.34.1",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.8",
    "xo": "^0.56.0"
  },
  "lint-staged": {
    "*.{vue,json,ts?(x)}": [
      "xo --fix",
      "prettier --write"
    ],
    "*.{y?(a)ml,?(m)js}": [
      "prettier --write"
    ]
  }
}
