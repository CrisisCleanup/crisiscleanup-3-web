{
  "name": "crisiscleanup",
  "version": "3.0.0-alpha89",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode production",
    "serve-dev": "cross-env NODE_ENV=development vue-cli-service serve --mode development",
    "build": "vue-cli-service build",
    "lint-fix": "vue-cli-service lint .",
    "lint-staged": "lint-staged",
    "lint": "vue-cli-service lint --no-fix  .",
    "prettify": "prettier --write {.,**}/*.js **/*.vue **/**/*.{vue,js}",
    "format": "yarn lint-fix && yarn prettify",
    "deploy-development": "vue-cli-service build --mode development && cross-env VUE_APP_STAGE=development vue-cli-service s3-deploy",
    "deploy-staging": "vue-cli-service build --mode production && cross-env VUE_APP_STAGE=staging vue-cli-service s3-deploy",
    "deploy-production": "vue-cli-service build --mode production && cross-env VUE_APP_STAGE=production vue-cli-service s3-deploy",
    "compile-icons": "./template-compiler.js",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "cross-env VUE_CLI_BABEL_TARGET_NODE=true VUE_CLI_BABEL_TRANSPILE_MODULES=true NODE_ENV=test vue-cli-service test:unit --coverage",
    "test:unit-fix": "yarn test:unit -u",
    "test:watch": "yarn test:unit --watchAll --silent --coverage=false",
    "test:e2e": "cross-env NODE_ENV=test cypress run",
    "test:e2e:hl": "yarn run test:e2e --headless",
    "test:cy": "cross-env NODE_ENV=test cypress run --browser chrome",
    "test:percy": "cross-env NODE_ENV=test percy exec -- yarn run test:cy",
    "cy:open": "cross-env NODE_ENV=test cypress open",
    "cy:patch": "node ./internal/scripts/patchCypress.js",
    "storybook": "cross-env NODE_ENV=storybook start-storybook -p 6006",
    "build-storybook": "cross-env NODE_ENV=storybook build-storybook",
    "postinstall": "yarn cache clean",
    "flow": "flow"
  },
