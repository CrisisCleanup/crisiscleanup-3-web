{
  "name": "crisiscleanup-4-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=16 <17",
    "pnpm": "<8"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/CrisisCleanup/crisiscleanup-4-web.git"
  },
  "scripts": {
    "prepare": "is-ci || husky install",
    "dev": "vite --port 8080",
    "build": "pnpm run typecheck && pnpm run build:app",
    "build:app": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "xo \"**/*.{vue,json,ts?(x)}\"",
    "lint:fix": "xo --fix \"**/*.{vue,json,ts?(x)}\"",
    "lint:errors": "pnpm run lint --quiet",
    "lint:format": "pnpm run lint:fix && pnpm run format",
    "format": "xo --prettier \"**/*.{vue,ts?(x),json,y?(a)ml}\"",
    "clean": "pnpm dlx rimraf dist \"**/coverage\"",
    "repair:clean-modules": "pnpm dlx rimraf node_modules",
    "repair:prune-pnpm": "pnpm run clean && pnpm run repair:clean-modules && pnpm store prune",
    "test": "vitest",
    "test:unit": "vitest",
    "test:e2e": "echo \"No e2e tests yet\" && exit 0",
    "test:cov": "vitest run --coverage"
  },
